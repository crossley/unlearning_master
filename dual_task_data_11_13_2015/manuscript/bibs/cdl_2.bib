@misc{_differential_????,
	title = {Differential dynamics of activity changes in dorsolateral and dorsomedial striatal loops during learning. ({PDF}) - {Semantic} {Scholar}},
	url = {https://www.semanticscholar.org/paper/Differential-dynamics-of-activity-changes-in-Thorn-Atallah/ee01f5e76a66b01f975f1f6d412c3e6dfb0241cf/pdf},
	abstract = {Article is made available in accordance with the publisher\&\#39;s policy and may be subject to US copyright law. Please refer to the publisher\&\#39;s site for terms of use. The MIT Faculty has made this article openly available. Please share how this access benefits you. Your story matters. SUMMARY The basal ganglia are implicated in a remarkable range of functions influencing emotion and cognition as well as motor behavior. Current models of basal ganglia function hypothesize that parallel limbic, associative, and motor cortico-basal ganglia loops contribute to this diverse set of functions, but little is yet known about how these loops operate and how their activities evolve during learning. To address these issues, we recorded simultaneously in sensorimotor and associative regions of the stria-tum as rats learned different versions of a conditional T-maze task. We found highly contrasting patterns of activity in these regions during task performance and found that these different patterns of structured activity developed concurrently, but with sharply different dynamics. Based on the region-specific dynamics of these patterns across learning, we suggest a working model whereby dorsomedial associa-tive loops can modulate the access of dorsolateral sensorimotor loops to the control of action.},
	urldate = {2016-08-28},
	file = {Snapshot:/Users/crossley/Library/Application Support/Firefox/Profiles/pitj7b4d.default/zotero/storage/5MNPIMZA/pdf.html:text/html}
}

