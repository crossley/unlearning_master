@article{aoki_role_2015,
	title = {Role of {Striatal} {Cholinergic} {Interneurons} in {Set}-{Shifting} in the {Rat}},
	volume = {35},
	issn = {0270-6474, 1529-2401},
	url = {http://www.jneurosci.org/content/35/25/9424},
	doi = {10.1523/JNEUROSCI.0490-15.2015},
	abstract = {The ability to change strategies in different contexts is a form of behavioral flexibility that is crucial for adaptive behavior. The striatum has been shown to contribute to certain forms of behavioral flexibility such as reversal learning. Here we report on the contribution of striatal cholinergic interneurons—a key element in the striatal neuronal circuit—to strategy set-shifting in which an attentional shift from one stimulus dimension to another is required. We made lesions of rat cholinergic interneurons in dorsomedial or ventral striatum using a specific immunotoxin and investigated the effects on set-shifting paradigms and on reversal learning. In shifting to a set that required attention to a previously irrelevant cue, lesions of dorsomedial striatum significantly increased the number of perseverative errors. In this condition, the number of never-reinforced errors was significantly decreased in both types of lesions. When shifting to a set that required attention to a novel cue, rats with ventral striatum lesions made more perseverative errors. Neither lesion impaired learning of the initial response strategy nor a subsequent switch to a new strategy when response choice was indicated by a previously relevant cue. Reversal learning was not affected. These results suggest that in set-shifting the striatal cholinergic interneurons play a fundamental role, which is dissociable between dorsomedial and ventral striatum depending on behavioral context. We propose a common mechanism in which cholinergic interneurons inhibit neurons representing the old strategy and enhance plasticity underlying exploration of a new rule.},
	language = {en},
	number = {25},
	urldate = {2016-08-21},
	journal = {The Journal of Neuroscience},
	author = {Aoki, Sho and Liu, Andrew W. and Zucca, Aya and Zucca, Stefano and Wickens, Jeffery R.},
	month = jun,
	year = {2015},
	pmid = {26109665},
	keywords = {behavioral flexibility, cholinergic interneuron, rat, set-shifting, striatum},
	pages = {9424--9431},
	file = {Full Text PDF:/Users/crossley/Library/Application Support/Firefox/Profiles/pitj7b4d.default/zotero/storage/FR5IBP5P/Aoki et al. - 2015 - Role of Striatal Cholinergic Interneurons in Set-S.pdf:application/pdf;Snapshot:/Users/crossley/Library/Application Support/Firefox/Profiles/pitj7b4d.default/zotero/storage/UG6VADWK/9424.html:text/html}
}

@article{baker_contralateral_2014,
	title = {Contralateral disconnection of the rat prelimbic cortex and dorsomedial striatum impairs cue-guided behavioral switching},
	volume = {21},
	issn = {1072-0502, 1549-5485},
	url = {http://learnmem.cshlp.org/content/21/8/368},
	doi = {10.1101/lm.034819.114},
	abstract = {Switches in reward outcomes or reward-predictive cues are two fundamental ways in which information is used to flexibly shift response patterns. The rat prelimbic cortex and dorsomedial striatum support behavioral flexibility based on a change in outcomes. The present experiments investigated whether these two brain regions are necessary for conditional discrimination performance in which a switch in reward-predictive cues occurs every three to six trials. The GABA agonists baclofen and muscimol infused into the prelimbic cortex significantly impaired performance leading rats to adopt an inappropriate turn strategy. The NMDA receptor antagonist D-AP5 infused into the dorsomedial striatum or prelimbic cortex and dorsomedial striatum contralateral disconnection impaired performance due to a rat failing to switch a response choice for an entire trial block in about two out of 13 test blocks. In an additional study, contralateral disconnection did not affect nonswitch discrimination performance. The results suggest that the prelimbic cortex and dorsomedial striatum are necessary to support cue-guided behavioral switching. The prelimbic cortex may be critical for generating alternative response patterns while the dorsomedial striatum supports the selection of an appropriate response when cue information must be used to flexibly switch response patterns.},
	language = {en},
	number = {8},
	urldate = {2016-08-21},
	journal = {Learning \& Memory},
	author = {Baker, Phillip M. and Ragozzino, Michael E.},
	month = aug,
	year = {2014},
	pmid = {25028395},
	pages = {368--379},
	file = {Full Text PDF:/Users/crossley/Library/Application Support/Firefox/Profiles/pitj7b4d.default/zotero/storage/355V5CP4/Baker and Ragozzino - 2014 - Contralateral disconnection of the rat prelimbic c.pdf:application/pdf;Snapshot:/Users/crossley/Library/Application Support/Firefox/Profiles/pitj7b4d.default/zotero/storage/GP3BWM7H/368.full.html:text/html}
}

